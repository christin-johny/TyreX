      #wrapper {
        display: flex;
      }

      .sidebar {
        width: 250px;
        min-height: 100vh; 
        margin-left: 0 !important; 
        padding-left: 0 !important;
        transition: all 0.3s ease;
      }
      
      .sidebar.toggled {
        width: 100px !important;
      }
      
      .sidebar.toggled .sidebar-brand-text {
        display: none;
      }
      
      .sidebar.toggled .nav-item .nav-link span {
        display: none;
      }
      
      /* Make the sidebar toggle button visible */
      #sidebarToggle {
        background-color: #4e73df; /* Primary theme color */
        color: white;
        width: 2.5rem;
        height: 2.5rem;
        text-align: center;
        line-height: 2.5rem;
        margin-bottom: 1rem;
        cursor: pointer;
        display: flex;
        justify-content: center;
        align-items: center;
        transition: background-color 0.3s;
        border-radius: 50%;
      }
      
      #sidebarToggle:hover {
        background-color: #2e59d9; /* Darker shade on hover */
      }
      
      #sidebarToggle i {
        font-size: 1rem;
        transition: transform 0.3s;
        color: white;
      }
      
      /* Make sure the button container is visible */
      .text-center.d-none.d-md-inline {
        display: inline-block !important;
      }
      
      /* For mobile toggle button */
      #sidebarToggleTop {
        color: #4e73df;
        border: none;
        background: transparent;
        font-size: 1.5rem;
        transition: color 0.3s;
      }
      
      #sidebarToggleTop:hover {
        color: #2e59d9;
      }
      
      @media (max-width: 768px) {
        .sidebar {
          width: 0;
          margin-left: -250px !important;
          position: fixed;
          z-index: 1030;
          height: 100%;
        }
        
        .sidebar.toggled {
          width: 250px !important;
          margin-left: 0 !important;
        }
        
        .sidebar.toggled .sidebar-brand-text,
        .sidebar.toggled .nav-item .nav-link span {
          display: inline;
        }
        
        #content-wrapper {
          margin-left: 0;
        }
        
        body.sidebar-toggled::after {
          content: "";
          position: fixed;
          top: 0;
          left: 0;
          width: 100%;
          height: 100%;
          background: rgba(0, 0, 0, 0.5);
          z-index: 1020;
        }
      }
      
      .scroll-to-top {
        position: fixed;
        right: 1rem;
        bottom: 1rem;
        display: none;
        width: 2.75rem;
        height: 2.75rem;
        text-align: center;
        color: #fff;
        background: rgba(90, 92, 105, 0.5);
        line-height: 46px;
        z-index: 99;
      }
      
      .scroll-to-top:hover {
        background-color: #5a5c69;
      }
      
      .scroll-to-top i {
        font-weight: 800;
      }
